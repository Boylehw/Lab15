import javax.swing.*;
import java.util.*;

public class RandomNumber {

  int CMove;	
  private int Lowbound;
  private int HighBound;


  	
  public int RandomNumber(){
   Lowbound=1;
   HighBound=10;
  	
  Random generator = new Random();
  return CMove = generator.nextInt(HighBound) + Lowbound;
  
  }
  
  public int RandomNumber(int LowBound,int HighBound){
  
  	Random generator = new Random();
  	return CMove = generator.nextInt(HighBound) + LowBound;
  
  }
  
  public int GetANumber(){

  
  	Random generator = new Random();
  	Lowbound = generator.nextInt(1000) + 0;
  	HighBound = generator.nextInt(Lowbound+1000) +(Lowbound+0);
  	return CMove = generator.nextInt(HighBound) + Lowbound;
  	
  	}

}
